<?xml version="1.0" encoding="utf-8"?>
<workbook>
	<worksheet name="Sheet1">
		<loop startRow="1" endRow="1" items="salesRecords" var="salesRecord"
			varType="com.wangf.sales.management.rest.pojo.SalesRecordPojo">
			<section startRow="1" endRow="1">
				<mapping row="1" col="1">salesRecord.hospital</mapping>
				<mapping row="1" col="2">salesRecord.salesPerson</mapping>
				<mapping row="1" col="6">salesRecord.product</mapping>
				<mapping row="1" col="7">salesRecord.installDepartment</mapping>
				<mapping row="1" col="8">salesRecord.orderDepartment</mapping>
				<mapping row="1" col="9">salesRecord.quantity</mapping>
			</section>
			<loopbreakcondition>
				<rowcheck offset="0">
					<cellcheck offset="0">End</cellcheck>
				</rowcheck>
			</loopbreakcondition>
		</loop>
	</worksheet>
</workbook>